		"@sveltejs/kit": "^1.0.0",
		"@types/cookie": "^0.5.1",
		"@types/node": "^20.4.5",
		"@typescript-eslint/eslint-plugin": "^6.5.0",
		"@typescript-eslint/parser": "^6.5.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.31.0",
		"postcss": "^8.4.26",
		"prettier": "^3.0.3",
